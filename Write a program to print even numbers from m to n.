#include <stdio.h>
int main()
{
    int m,n,num,i;
    printf("Enter the starting number(m):");
    scanf("%d",&m);
    printf("Enter the ending number(n):");
    scanf("%d",&n);
    if(m%2==0)
    {
        num=m;
    }
    else
    {
        num=m+1;
    }
    printf("\nEven numbers:\n");
    for(i=num;i<=n;i=i+2)
    {
        printf("%d\n",i);
    }
    return 0;
}
