#include <stdio.h>
int factorial(int);
int main()
{
    int n,f;
    printf("Enter the number(n):");
    scanf("%d",&n);
    f=factorial(n);
    printf("Factorial of the number:%d",f);
    return 0;
}
int factorial(int n)
{
    int i,f=1;
    for(i=1;i<=n;i++)
    {
        f=f*i;
    }
    return f;
}
